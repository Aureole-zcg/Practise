/*1．编写程序,程序运行时输出以下图形。
****
 ****
  ****
   ****
*/
#include<stdio.h>
int main()
{
	printf("****\n ****\n  ****\n   ****\n");
	return 0;
}
-------------------------------------------------
/*1．编写程序,程序运行时输出以下图形。
****
 ****
  ****
   ****
*/
#include<stdio.h>
int main()
{
	int i, j;
	for (i = 1; i < 5; i++)
	{
		for (j = 1;j < 5;j++)
			printf("*");
		printf("\n");
		if (i == 1) printf(" ");
		if (i == 2) printf("  ");
		if (i == 3) printf("   ");
	}
	return 0;
}
